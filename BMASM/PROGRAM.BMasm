.ORG &0000
Start:
	INTR

GETKEY:
	INBY 20
	OUTB 00
	CMPC ESC
	JINT GETKEY

	HLTC
	
PRINT:
	INCR #00
PRINT_LOOP:
	CMPA #00
	POPR #02
	INBY #00
	DECR #00
	JINZ PRINT_LOOP
	INBY #10
	RESR

; THIS IS THE INT ADDR
.ORG &1000
	PUHS "HELLO WORLD!"
	LODA #10
	CALL PRINT
	REIN

.ORG &FF00
.word &1000

.ORG &FFFE
	JUMP Start